{
  "name": "saveFeedback",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: blum-serverless",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 512,
  "authorizer": {},
  "custom": {
    "optimize": {
      "exclude": [ "aws-sdk" ],
      "transforms": [
        {
          "name": "babelify",
          "opts": {
            "presets": [
              "es2015"
            ]
          }
        }
      ]
    }
  },
  "endpoints": [
    {
      "path": "user/exercise/{exercise_id}/feedback",
      "method": "PUT",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "machineUserAuth",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${putFeedbackTemplate}",
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "404": {
          "statusCode": "404",
          "selectionPattern": ".*\"status\":404.*",
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": "$input.path('$.errorMessage')"
          }
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "SERVERLESS_DATABASE": "${database}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}